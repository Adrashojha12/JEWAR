            $('#countryList').fadeOut();

        });

        $(document).on('click', '.search_project_city', function() {

            $('#city_name1').val($(this).text());

            $('#cityList').fadeOut();

        });

    });

    $(document).on('click', '.skip', function() {

        var min_price = $('#min_price').val();

        var max_price = $('#max_price').val();


