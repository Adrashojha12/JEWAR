<script>
    function change_city(city_id) {

        $('#city_id').val(city_id);

        priceupdate();

    }

    function priceupdate() {

        var _token = $('input[name="_token"]').val();

        var city_slug = $('#city_id').val();

        $.ajax({

            url: "https://www.omaxe.com/projects/getminmaxprice",

            method: "POST",

            data: {

                query: city_slug,

                _token: _token

            },

            success: function(data) {

                //$('#min_price').val(data.min_price);

                //$('#max_price').val(data.max_price);

                //alert(data)

                //console.log(data);

                //$('#countryList').fadeIn();

                //$('#countryList').html(data);

            }

        });

    }

    $(function() {

        $("#slider-range").slider({

            range: false,

            type: "single",

            orientation: "horizontal",

            min: 0,

            max: 80000000.00,

            values: [0, 80000000.00],

            step: 100,

            slide: function(event, ui) {

                if (ui.values[0] == ui.values[1]) {

                    return false;

                }

                $("#min_price").val(ui.values[0]);

                $("#max_price").val(ui.values[1]);

            }

        });

        $("#min_price").val($("#slider-range").slider("values", 0));

        $("#max_price").val($("#slider-range").slider("values", 1));

    });

    $(document).ready(function() {

        $('#city_name').keyup(function() {

            var query = $(this).val();

            //alert(query);

            if (query != '') {

                var _token = $('input[name="_token"]').val();

                //alert(_token);

                $.ajax({

                    url: "https://www.omaxe.com/autocomplete/fetch",

                    method: "POST",

                    data: {

                        query: query,

                        _token: _token

                    },

                    success: function(data) {

                        $('#countryList').fadeIn();

                        $('#countryList').html(data);

                    }

                });

            }

        });
        $('#city_name').keydown(function() {
            $('#countryList').fadeOut();
        });

        $('#city_name1').keyup(function() {

            var query = $(this).val();

            //alert(query);

            if (query != '') {

                var _token = $('input[name="_token"]').val();

                //alert(_token);

                $.ajax({

                    url: "https://www.omaxe.com/autocomplete/fetchSearchCity",

